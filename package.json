  "scripts": {
    "dev": "NODE_ENV=development useFirebaseEmulator=false node app.js --hookTimeout=160000",
    "dev-emulator": "NODE_ENV=development useFirebaseEmulator=true node app.js --hookTimeout=160000",
    "start_local": "redis-server 2>&1 & NODE_ENV=production node app.js --silly --hookTimeout=160000",
    "start": "NODE_ENV=production node app.js --silly --hookTimeout=160000",
    "start_profile": "redis-server 2>&1 & source /usr/local/nsolid/nsolid-env && NODE_ENV=production node app.js --silly --hookTimeout=160000",
    "tsc": "tsc ./assets/js",
    "sails": "$(npm bin)/sails",
    "lift": "$(npm bin)/sails lift",
    "lint": "./node_modules/eslint/bin/eslint.js . --max-warnings=0 --report-unused-disable-directives && echo 'âœ”  Your .js files look good.'",
    "test": "clear && npm run lint && npm run custom-tests && echo 'Done.'",
    "custom-tests": "clear && node ./node_modules/mocha/bin/mocha --exit -t 20000 --bail --require ts-node/register --colors test/lifecycle.test.js test/integration/**/*.test.js test/integration/**/*.test.ts",
    "test-interfaces": "clear && node ./node_modules/mocha/bin/mocha --exit --require ts-node/register test/units/**/*.test.ts",
    "heroku-postbuild": "grunt build",
    "clean": "rm -rf .tmp/bootstrap-version.json && node app.js --drop"
  },
