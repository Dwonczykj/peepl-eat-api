{
    "path-intellisense.showHiddenFiles": true,
    "prettier.tabWidth": 2,
    "files.exclude": {
        "**/.git": true,
        "**/.sass-cache": true,
        "**/.assets_babel": true,
        "**/node_modules": true,
        "**/.tscache": false,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "package-lock.json": true,
    },
    "editor.tokenColorCustomizations": {
        "comments": "",
        "textMateRules": []
    },
    "workbench.colorCustomizations": {
        "titleBar.activeBackground": "#546bf0df",
        "titleBar.inactiveBackground": "#050a50",
        "titleBar.activeForeground": "#000",
        "tab.activeBackground": "#ff002b59",
        "tab.activeBorder": "#8c1cf5cb",
        "tab.activeBorderTop": "#eeff01f6",
    },
}